<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="SpinnerDropDownItem" parent="Theme.AppCompat.Light">
        <item name="android:background">#595555</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:padding">10dp</item>
        <item name="android:layout_margin">2dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxLines">1</item>
        <item name="android:ellipsize">end</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_marginBottom">2dp</item>
        <item name="android:layout_marginStart">10dp</item>
        <item name="android:layout_marginEnd">10dp</item>
        <item name="android:outlineSpotShadowColor">@android:color/transparent</item>
        <item name="android:outlineAmbientShadowColor">@android:color/transparent</item>
        <item name="android:outlineProvider">@null</item>
        <item name="android:stateListAnimator">@null</item>
        <item name="android:insetLeft">5dp</item>
        <item name="android:insetRight">5dp</item>
        <item name="android:insetTop">2dp</item>
        <item name="android:insetBottom">2dp</item>
        <item name="android:backgroundTintMode">src_in</item>
    </style>
</resources>